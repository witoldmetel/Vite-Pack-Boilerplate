{
  "name": "ts-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Starter pack for building TypeScript apps",
  "main": "index.js",
  "scripts": {
    "start": "yarn clean && parcel public/index.html --out-dir development -p 8080",
    "build": "yarn lint && yarn test && parcel build public/index.html --no-cache --out-dir dist --public-url ./",
    "deploy": "gh-pages -b master -d dist",
    "clean": "rimraf ./development && rimraf -rf ./.cache",
    "lint": "eslint --fix src/*",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/witoldmetel/TS-Parcel-Boilerplate.git"
  },
  "keywords": [
    "parcel",
    "typescript",
    "react",
    "sass",
    "boilerplate"
  ],
  "author": "Witold Metel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/witoldmetel/TS-Parcel-Boilerplate/issues"
  },
  "homepage": "https://witoldmetel.github.io/TS-Parcel-Boilerplate",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/runtime-corejs2": "7.9.2",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.2",
    "@types/p5": "0.9.0",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "babel-jest": "25.5.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-require-path-exists": "1.1.9",
    "firebase": "^7.18.0",
    "jest": "25.5.4",
    "p5": "1.0.0",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "sass": "1.26.3",
    "ts-jest": "25.4.0"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "3.8.3"
  }
}
